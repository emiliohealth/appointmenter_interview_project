# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep
/db/*.sqlite3
/db/*.sqlite3-journal
/db/*.sqlite3-[0-9]*

# Ignore uploaded files in development.
/storage/*
!/storage/.keep

/public/assets
.byebug_history

# Ignore master key for decrypting credentials and more.
/config/master.key

/public/packs
/public/packs-test
/node_modules
/yarn-error.log
yarn-debug.log*
.yarn-integrity

# Ignore env files
.env
.env.local
.env.test.local
.env-frontend

config/settings.local.yml
config/settings/*.local.yml
config/environments/*.local.yml

Brewfile.lock.json

# ignore git commit templates, since they may vary by individual
.gitmessage

erd.pdf
*.dump
.DS_Store
